	<servlet-class>com.mine.servlet.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>login</servlet-name>
	<url-pattern>/login</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>login</servlet-name>
